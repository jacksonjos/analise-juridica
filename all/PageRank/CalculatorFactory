class CalculatorFactory(){

    public Calculator getCalculator( Integer mode, DBCollection inCollection, DBCollection outCollection){
        if( mode =='1'){
            return new CalculatorPR1( DBCollection inCollection, DBCollection outCollection);
        }
        if( mode =='2'){
            return new CalculatorPG2( DBCollection inCollection, DBCollection outCollection);
        }
    }
}
